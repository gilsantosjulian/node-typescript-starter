{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-eslint-rules"
    ],
    "rules": {
        "no-console": {"options":[true,"log","error"],"severity":"warning"},
        "object-literal-sort-keys":{"options":[false],"severity":"warning"},
        "array-type": {"options":["array"],"severity":"warning"},
        "max-line-length": {"options":[90],"severity":"warning"},
        "no-unused-expression": {"options":[true],"severity":"warning"},
        "ordered-imports": {"options":[true],"severity":"warning"}
    }
}