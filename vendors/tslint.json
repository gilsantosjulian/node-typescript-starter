{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-eslint-rules"
    ],
    "rules": {
        "object-literal-sort-keys":false,
        "max-line-length": {"options":[90],"severity":"warning"},
        "comment-format":[true, "check-space"],
        "trailing-comma": {"options":[true,"always"],"severity":"error"}
    }
}